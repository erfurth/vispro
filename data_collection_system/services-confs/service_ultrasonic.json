            "input": "opcua",
            "output": "mqtt"
        }
    },
    "data_source": {
        "name": "ultrasonic",
        "oranisation": "private",
        "connection": {
            "host": "host.docker.internal",
            "port": 4841,
            "security": {
                "policy": "None",
                "mode": "None"
            }
        },
        "reading_rate": 0.5
    },
    "data_sink": {
        "name": "mqtt-broker",
