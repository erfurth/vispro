{
    "mqtt_broker": {
        "hostname": "{{mosquitto_host_address}}",
        "port": {{mosquitto_port}},
        "keepalive": 60
    }, 
    "mongodb": {
        "host": "mongodb",
        "user_name": "{{mongodb_user_name}}",
        "password": "{{mongodb_password}}"
    },
    "topics": [
        {%- for service in data_services %}
        "{{service.name}}-{{service.input_protocol}}/static"{% if not loop.last %},{% endif %}
        {%- endfor %}
    ]
}